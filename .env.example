# Geetanjali Environment Configuration

# Database
POSTGRES_DB=geetanjali
POSTGRES_USER=geetanjali
POSTGRES_PASSWORD=change_in_production

# Application
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Ollama (runs on host, not in Docker)
# Make sure Ollama is running: brew services start ollama
# Model is already pulled: ollama list
OLLAMA_MODEL=llama3.2:3b
